<!-- Issue tracker connector configs-->
    <IssueTrackerConnector name="redmine">
        <Property name="Url">https://redmine.<%= domain %></Property>
        <Property name="AdminUsername"><%= redmine_admin_uname %></Property>
        <Property name="AdminPassword"><%= redmine_admin_passwd %></Property>
        <Property name="DefaultRole">Developer</Property>
        <Property name="AuthenticatorId">1</Property>
