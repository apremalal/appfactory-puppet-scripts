            </Role>
            <Role name="developer">
                <RedmineRole>Developer</RedmineRole>
            </Role>
            <Role name="QA">
                <RedmineRole>Reporter</RedmineRole>
            </Role>
            <Role name="devOps">
                <RedmineRole>Reporter</RedmineRole>
            </Role>
        </RoleMap>
